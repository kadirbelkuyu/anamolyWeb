{
    "name": "payvision/payvision-sdk-php",
    "description": "Payvision PHP SDK",
    "type": "library",
    "version": "4.1.0",
    "license": "MIT",
    "require": {
        "php": "^7.0.13|^7.1",
        "ext-simplexml": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^6.3",
        "firebase/php-jwt": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Payvision\\SDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Payvision\\SDK\\Test\\": "tests/Test"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    }
}
